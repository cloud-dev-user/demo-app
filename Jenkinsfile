pipeline{  
                 agent any; 
                 stages { 
                             stage("checkout my code") {
                                        steps {
                                                   echo " checkout my code " 
                                                }
                                        }
                                stage ("build my code") {
                                       steps{
                                                  echo " build my code " 
                                               }
                                         }
                                 stage ("test my code") {
                                       steps{
                                                  echo " test my code " 
                                               }
                                         }
                          }
     }
